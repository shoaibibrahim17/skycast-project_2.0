{
  "expo": {
    "name": "SkyCast",
    "slug": "skycast-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon(2).png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.skycast",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/skycast_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourname.skycast",
      "versionCode": 1
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show current weather.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show current weather."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5d4b4b83-8da2-4f61-bfa1-052e4a2d85c3"
      }
    }
  }
}
